{
  "name": "markdown",
  "displayName": "Markdown",
  "description": "Markdown Paste Image To Qiniu OSS store",
  "version": "1.1.0",
  "publisher": "starkwang",
  "icon": "images/icon.png",
  "author": {
    "name": "starkwang",
    "email": "wsd312@163.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wsdo/vscode-markdown-img-qiniu.git"
  },
  "homepage": "https://github.com/wsdo/vscode-markdown-img-qiniu.git/blob/master/README.md",
  "bugs": {
    "url": "https://github.com/wsdo/vscode-markdown-img-qiniu.git/issues"
  },
  "engines": {
    "vscode": "^1.11.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.qiniu"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.qiniu",
        "title": "Paste Image To Qiniu"
      }
    ],
    "keybindings": [
      {
        "command": "extension.qiniu",
        "key": "ctrl+alt+v",
        "mac": "cmd+alt+v",
        "when": "editorTextFocus"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "qiniu configuration",
      "properties": {
        "qiniu.access_key": {
          "type": "string",
          "default": "",
          "description": "一个有效的七牛 AccessKey 签名授权。"
        },
        "qiniu.secret_key": {
          "type": "string",
          "default": "",
          "description": "一个有效的七牛 SecretKey 签名授权。"
        },
        "qiniu.bucket": {
          "type": "string",
          "default": "",
          "description": "七牛图片上传空间。"
        },
        "qiniu.remotePath": {
          "type": "string",
          "default": "${fileName}",
          "description": "七牛图片上传路径，参数化命名。"
        },
        "qiniu.domain": {
          "type": "string",
          "default": "",
          "description": "七牛图床域名。"
        },
        "qiniu.isSaveLocal": {
          "type": "boolean",
          "default": false,
          "description": "图片是否保存本地"
        },
        "qiniu.localPath": {
          "type": "string",
          "default": "./img",
          "description": "图片本地保存位置"
        }
      }
    }
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "eslint": "^3.6.0",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "moment": "^2.18.1",
    "qiniu": "^6.1.13"
  }
}
